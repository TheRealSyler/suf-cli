{
  "name": "suf-cli",
  "version": "0.3.2",
  "description": "Utility Cli",
  "scripts": {
    "prepack": "yarn build && node ./dist/index.js",
    "build": "del-cli ./dist && rollup --config rollup.config.ts --configPlugin typescript",
    "watch": "tsc -w",
    "test": "jest --coverage",
    "test:clear": "jest --clearCache",
    "test:watch": "jest --watch --coverage"
  },
  "author": "Syler",
  "license": "MIT",
  "bin": {
    "suf": "./dist/index.js"
  },
  "files": [
    "dist/src",
    "dist/index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TheRealSyler/suf-cli"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "suf-log": "^2.5.3",
    "suf-node": "^1.3.3",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/node-fetch": "^2.6.1",
    "del-cli": "^4.0.1",
    "jest": "^28.1.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-process": "^1.4.1",
    "jest-store-log": "^1.3.0",
    "mock-stdin": "^1.0.0",
    "package-json-type": "^1.0.3",
    "rollup": "^2.72.1",
    "rollup-plugin-hashbang": "^3.0.0",
    "ts-jest": "^28.0.2"
  }
}